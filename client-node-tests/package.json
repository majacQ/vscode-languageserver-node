{
	"name": "text-extension",
	"displayName": "text-extension",
	"private": true,
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {},
	"scripts": {
		"clean": "node ../node_modules/rimraf/bin.js lib",
		"compile": "node ../build/bin/tsc -b ./tsconfig.json",
		"watch": "node ../build/bin/tsc -b ./tsconfig-watch.json -w",
		"lint": "node ../node_modules/eslint/bin/eslint.js ./src/**/*.ts",
		"test": "node ../build/bin/symlink-tests.js && node lib/runTests.js"
	},
	"dependencies": {
		"minimatch": "^3.0.5",
		"vscode-languageclient": "7.1.0-next.1",
		"vscode-languageserver": "7.1.0-next.1",
		"vscode-uri": "2.1.1"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/minimatch": "^3.0.3",
		"@types/sinon": "^7.0.13",
		"@types/uuid": "^7.0.2",
		"@types/vscode": "1.53.0",
		"glob": "^7.1.4",
		"sinon": "^10.0.0",
		"uuid": "7.0.3",
		"vscode-test": "^1.2.0"
	}
